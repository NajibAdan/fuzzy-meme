<h1>List of users</h1>
<div class="text-center"> <%= will_paginate%> </div>
<ul class="users">
<%@users.each do |user|%>
    <li> <%= link_to user.name, user_path(user) %> | 
    <%= link_to 'Delete', admin_destroy_user_path(user), method: :delete,data: {confirm: "Are you sure?"} , class:'alert-danger'%>
    </li>
<%end%>
</ul>
<div class="text-center"> <%= will_paginate%> </div>